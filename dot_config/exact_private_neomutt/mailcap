text/html; /usr/bin/w3m -dump -T text/html -I %{charset} -O utf-8 %s; copiousoutput; description=HTML Text; nametemplate=%s.html

application/msword; catdoc %s; copiousoutput
application/vnd.ms-powerpoint; catppt %s; copiousoutput
application/vnd.ms-excel; xlsx2csv -c utf-8 %s -; copiousoutput
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; xlsx2csv -c utf-8 %s -; copiousoutput
application/vnd.openxmlformats-officedocument.presentationml.presentation; pptx2txt.sh %s; copiousoutput;

application/pdf; zathura %s 2>/dev/null; test=test -n "$DISPLAY";
application/pdf; pdftotext %s -; copiousoutput

image/*; feh -x. %s; test=test -n "$DISPLAY"
